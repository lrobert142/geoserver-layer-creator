<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import java.net.*?>

<BorderPane minHeight="740" minWidth="720.0" prefHeight="740" prefWidth="720.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="au.gov.aims.view.RootLayoutController">
      <stylesheets>
  <URL value="@BlueStyle.css" />
</stylesheets>
   <top>
      <MenuBar styleClass="menu-bar" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" styleClass="menu-bar .label" text="File">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#ChangeDefaultDirectory" text="Set Default Directory Path" />
                  <MenuItem mnemonicParsing="false" onAction="#openServerDetailsView" text="Set Server Details" />
              <MenuItem mnemonicParsing="false" onAction="#close" styleClass=".menu-bar .menu-item" text="Close" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>

</BorderPane>
